{
  "name": "langgraph-service-adapter",
  "version": "0.1.0",
  "description": "A standalone service adapter for integrating LangGraph with CopilotKit runtime",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "prepare": "tsup",
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "biome check",
    "lint:fix": "biome check --write --unsafe",
    "check-types": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm run clean && pnpm run build && pnpm run test"
  },
  "keywords": [
    "langgraph",
    "copilotkit",
    "service-adapter",
    "ai",
    "langchain",
    "typescript",
    "llm",
    "chatbot",
    "agent",
    "streaming",
    "tools",
    "function-calling"
  ],
  "author": "LangGraph Service Adapter Contributors",
  "license": "MIT",
  "packageManager": "pnpm@10.15.1",
  "peerDependencies": {
    "@copilotkit/runtime": "^1.10.0"
  },
  "dependencies": {
    "@copilotkit/shared": "^1.10.0",
    "@langchain/core": "^0.3.58",
    "@langchain/langgraph": "^0.4.9",
    "langchain": "^0.3.3",
    "class-transformer": "^0.5.1",
    "rxjs": "^7.8.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^18.19.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "tsup": "^8.0.2",
    "typescript": "^5.2.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/langgraph-service-adapter.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/langgraph-service-adapter/issues"
  },
  "homepage": "https://github.com/your-org/langgraph-service-adapter#readme",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/your-org"
  }
}
